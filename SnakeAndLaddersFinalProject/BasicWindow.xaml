<Window x:Class="SnakeAndLaddersFinalProject.BasicWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SnakeAndLaddersFinalProject"
        mc:Ignorable="d"
        Title="BasicWindow" Height="450" Width="800"
        Loaded="Window_Loaded">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/Assets/Images/BackgroundMainWindow.png"
                  Stretch="UniformToFill"/>
        </Grid.Background>
        <Frame x:Name="MainFrame" NavigationUIVisibility="Hidden" Background="Transparent"/>
    </Grid>
</Window>
