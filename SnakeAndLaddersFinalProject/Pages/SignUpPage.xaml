<Page x:Class="SnakeAndLaddersFinalProject.Pages.SignUpPage" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:local="clr-namespace:SnakeAndLaddersFinalProject.Pages" 
      xmlns:language="clr-namespace:SnakeAndLaddersFinalProject.Properties.Langs" mc:Ignorable="d" d:DesignHeight="450" d:DesignWidth="800" Title="SignUpPage">
    <Grid>
        <Button x:Name="btnSettings" Content="{x:Static language:Lang.btnSettingsText}" HorizontalAlignment="Left" Margin="691,30,0,0" VerticalAlignment="Top" Height="24" Width="79"/>
        <Label x:Name="lblTitleSignUp" Content="{x:Static language:Lang.btnSignUpText}" HorizontalAlignment="Left" Margin="164,30,0,0" VerticalAlignment="Top" Height="24" Width="432"/>
        <Label x:Name="lblUsername" Content="{x:Static language:Lang.txtUsernameText}" HorizontalAlignment="Left" Margin="120,194,0,0" VerticalAlignment="Top" Height="26" Width="170"/>
        <Label x:Name="lblPassword" Content="{x:Static language:Lang.pwdPasswordText}" HorizontalAlignment="Left" Margin="527,193,0,0" VerticalAlignment="Top" Height="29" Width="170"/>
        <Label x:Name="lblNameOfUser" Content="{x:Static language:Lang.txtNameOfUserText}" HorizontalAlignment="Left" Margin="120,88,0,0" VerticalAlignment="Top" Height="31" Width="180"/>
        <Label x:Name="lblLastName" Content="{x:Static language:Lang.txtLastNameOfUserText}" HorizontalAlignment="Left" Margin="527,88,0,0" VerticalAlignment="Top" Height="26" Width="170"/>
        <Label x:Name="lblEmail" Content="{x:Static language:Lang.txtRegEmailText}" HorizontalAlignment="Left" Margin="120,308,0,0" VerticalAlignment="Top" Height="27" Width="170"/>
        <TextBox x:Name="txtNameOfUser" HorizontalAlignment="Left" Margin="120,117,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="180" Height="29"/>
        <TextBox x:Name="txtLastname" HorizontalAlignment="Left" Margin="527,114,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="170" Height="29"/>
        <TextBox x:Name="txtUsername" HorizontalAlignment="Left" Margin="120,222,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="180" Height="31"/>
        <TextBox x:Name="txtEmail" HorizontalAlignment="Left" Margin="120,340,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="280" Height="32"/>
        <PasswordBox x:Name="pwdPassword" HorizontalAlignment="Left" Margin="527,222,0,0" VerticalAlignment="Top" Width="170" Height="31"/>
        <Button x:Name="btnSignUp" Content="{x:Static language:Lang.btnSignUpText}" HorizontalAlignment="Left" Margin="542,343,0,0" VerticalAlignment="Top" Height="26" Width="140"/>
        <Button x:Name="btnBack" Content="{x:Static language:Lang.btnBackText}" HorizontalAlignment="Left" Margin="30,30,0,0" VerticalAlignment="Top" Height="24" Width="91"/>
    </Grid>
</Page>